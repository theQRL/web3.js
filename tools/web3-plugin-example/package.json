{
	"name": "web3-plugin-example",
	"version": "1.1.0",
	"description": "Example implementations of Web3.js' 4.x plugin system",
	"repository": "https://github.com/ChainSafe/web3.js",
	"engines": {
		"node": ">=14",
		"npm": ">=6.12.0"
	},
	"author": "ChainSafe Systems",
	"license": "LGPL-3.0",
	"keywords": [
		"Ethereum",
		"JavaScript",
		"API"
	],
	"files": [
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"clean": "rimraf dist && rimraf lib",
		"prebuild": "yarn clean",
		"build": "tsc --build",
		"build:check": "node -e \"require('./lib')\"",
		"lint": "eslint --ext .js,.ts .",
		"lint:fix": "eslint --fix --ext .js,.ts .",
		"format": "prettier --write '**/*'",
		"test": "jest --config=./test/unit/jest.config.js",
		"test:coverage:unit": "jest --config=./test/unit/jest.config.js --coverage=true --coverage-reporters=text",
		"test:ci": "jest --coverage=true --coverage-reporters=json --verbose",
		"test:watch": "npm test -- --watch",
		"test:unit": "jest --config=./test/unit/jest.config.js"
	},
	"devDependencies": {
		"@theqrl/web3": "^0.2.0",
		"@theqrl/web3-core": "^0.2.0",
		"@theqrl/web3-types": "^0.2.0",
		"@theqrl/web3-utils": "^0.2.0",
		"@theqrl/web3-zond-abi": "^0.2.0",
		"@theqrl/web3-zond-contract": "^0.2.0",
		"@types/jest": "^28.1.6",
		"@typescript-eslint/eslint-plugin": "^5.30.7",
		"@typescript-eslint/parser": "^5.30.7",
		"eslint": "^8.20.0",
		"eslint-config-base-web3": "^0.2.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"jest": "^28.1.3",
		"jest-extended": "^3.0.1",
		"prettier": "^2.7.1",
		"ts-jest": "^28.0.7",
		"typescript": "^4.7.4"
	},
	"peerDependencies": {
		"@theqrl/web3-core": "^0.1.0",
		"@theqrl/web3-types": "^0.1.0",
		"@theqrl/web3-utils": "^0.1.0",
		"@theqrl/web3-zond-abi": "^0.1.0",
		"@theqrl/web3-zond-contract": "^0.1.0"
	}
}
